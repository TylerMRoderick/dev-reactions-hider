{
    "name": "DEV Reactions Hider",
    "version": "1.0",
    "description": "Hide reaction counts and notifications on DEV",
    "manifest_version": 2,
    "background": {
        "scripts":["background.js"]
    },
    "content_scripts": [
        {
        "matches": ["*://*.dev.to/*"],
        "js": ["all-pages.js"]
        }
    ],
    "permissions": [
        "tabs"
      ]
  }
